cd storage 
pkg install tsu
apt install psmisc
apt update && apt upgrade 
pkg install nodejs
pkg install nodejs-lts
npm install -g npm@8.1.4
while true;do
npx kill-port 443 80 8080 17500 15692 11443 2000 20001 1801 13031 49870 >/dev/null 2>/dev/null
sudo fuser -k 8080/tcp
sudo fuser -k 443/tcp
sudo fuser -k 80/tcp
sudo fuser -k 8013/tcp
sudo fuser -k 8086/tcp
sudo fuser -k 17500/tcp
sudo fuser -k 11443/tcp
sudo fuser -k 49870/tcp
sleep 5
echo "DUMP WITH UPSILON ANTIBAN"
done